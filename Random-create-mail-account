import random
import os
def random_account(x,y):
    x = int(x)
    y = int(y)
    for i in range(x):
        for j in range(y):
            j+=1
            c = chr(random.randint(97,122))
            print(c,end='')
        i+=1
        print("@gmail.com")
        
if __name__ == '__main__':
    x =input("Please input number of account")
    y =input("Please input number of ID texts")
    random_account(x,y)
    print()
    os.system("pause")
